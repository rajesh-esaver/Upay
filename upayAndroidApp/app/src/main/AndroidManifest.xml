<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.calendar.upay">

    <uses-permission
        android:name="android.permission.INTERNET "/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".shoppurchase.PurchaseAcceptActivity" />
        <activity android:name=".shoppurchase.ShopPurchaseActivity" />
        <activity android:name=".userpayment.UserPaymentActivity" />
        <activity android:name=".profile.ProfileActivity" />
        <activity android:name=".transactions.TransactionDetailsActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>